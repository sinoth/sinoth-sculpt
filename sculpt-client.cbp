<?xml version="1.0" encoding="UTF-8" standalone="yes" ?>
<CodeBlocks_project_file>
	<FileVersion major="1" minor="6" />
	<Project>
		<Option title="sculpt-client" />
		<Option pch_mode="2" />
		<Option compiler="gcc" />
		<Build>
			<Target title="Windows Client Debug">
				<Option platforms="Windows;" />
				<Option output="bin\client\sculpt_d" prefix_auto="1" extension_auto="1" />
				<Option working_dir="res" />
				<Option object_output="bin\client\obj\Debug\" />
				<Option type="1" />
				<Option compiler="gcc" />
				<Option parameters="localhost" />
				<Compiler>
					<Add option="-g" />
					<Add option="-D_WIN32_WINNT=0x0501" />
					<Add directory="..\..\libs\boost\include" />
				</Compiler>
				<Linker>
					<Add library="..\..\libs\boost\lib\liblibboost_system-gcc44-mt-d-1_40.a" />
					<Add library="ws2_32" />
				</Linker>
			</Target>
			<Target title="Windows Client Release">
				<Option platforms="Windows;" />
				<Option output="bin\client\sculpt" prefix_auto="1" extension_auto="1" />
				<Option working_dir="res" />
				<Option object_output="bin\client\obj\Release\" />
				<Option type="0" />
				<Option compiler="gcc" />
				<Compiler>
					<Add option="-O2" />
				</Compiler>
				<Linker>
					<Add option="-s" />
				</Linker>
			</Target>
		</Build>
		<Compiler>
			<Add option="-Wall" />
		</Compiler>
		<Unit filename="client\main.cpp" />
		<Extensions>
			<code_completion />
			<envvars />
			<debugger />
			<lib_finder disable_auto="1" />
		</Extensions>
	</Project>
